{
    "manifest_version": 2,
    "name": "YNAB Percentage view",
    "version": "1.1",
    "content_scripts": [
        {
            "matches": [
                "https://app.youneedabudget.com/*/budget*"
            ],
            "js": [
                "index.js"
            ]
        }
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "ynab_percentage_webextension@sylvain.dev",
            "strict_min_version": "52.0"
        }
    },
    "icons": {
        "192": "icons/192x192.png",
        "57": "icons/57x57.png",
        "60": "icons/60x60.png",
        "72": "icons/72x72.png",
        "76": "icons/76x76.png",
        "114": "icons/114x114.png",
        "120": "icons/120x120.png",
        "144": "icons/144x144.png",
        "152": "icons/152x152.png",
        "180": "icons/180x180.png"
    }
}
